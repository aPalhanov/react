    "faker": "^5.5.3",
    "firebase-admin": "^9.9.0",
    "forcem-ipsum": "^1.7.3",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "isomorphic-fetch": "^3.0.0",
    "json-server": "^0.16.3",
